<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="446.0" prefWidth="312.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.librarymanagementsystem.controller.StudentRegisterController">
   <children>
      <Label layoutX="27.0" layoutY="14.0" text="Student Register Form">
         <font>
            <Font name="Comic Sans MS" size="24.0" />
         </font>
      </Label>
      <TextField fx:id="txtYear" layoutX="27.0" layoutY="230.0" prefHeight="25.0" prefWidth="259.0" promptText="Year">
         <font>
            <Font name="Comic Sans MS" size="15.0" />
         </font>
      </TextField>
      <TextField fx:id="txtName" layoutX="27.0" layoutY="82.0" prefHeight="25.0" prefWidth="259.0" promptText="Name">
         <font>
            <Font name="Comic Sans MS" size="15.0" />
         </font>
      </TextField>
      <TextField fx:id="txtRollNo" layoutX="27.0" layoutY="154.0" prefHeight="25.0" prefWidth="259.0" promptText="Roll Number">
         <font>
            <Font name="Comic Sans MS" size="15.0" />
         </font>
      </TextField>
      <TextField fx:id="txtAcademicYear" layoutX="27.0" layoutY="302.0" prefHeight="25.0" prefWidth="259.0" promptText="Academic Year">
         <font>
            <Font name="Comic Sans MS" size="15.0" />
